class speak:
    def sound(self):
        pass

class Dog(speak):
    def sound(self):
        print("bark ğŸ¶")

class Cat(speak):
    def sound(self):
        print("meow ğŸ˜º")

class Snake(speak):
    def sound(self):
        print("hiss ğŸ")
        
class Wolf(speak):
    def sound(self):
        print("howl ğŸº")
        
class Hyena(speak):
    def sound(self):
        print("laugh ğŸ˜‚")

# Test the animals speaking
speak = [Dog(), Cat(), Snake(), Wolf (), Hyena ()]
for speak in speak:
    speak.sound()
